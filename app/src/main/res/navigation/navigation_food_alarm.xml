<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigationFoodAlarm"
    app:startDestination="@id/foodAlarmFragment">

    <dialog
        android:id="@+id/foodAlarmFragment"
        android:name="com.example.mypet.ui.food.alarm.FoodAlarmFragment"
        tools:layout="@layout/fragment_food_alarm">
        <argument
            android:name="petMyId"
            app:argType="integer" />
        <argument
            android:name="petFoodId"
            app:argType="integer" />
        <action
            android:id="@+id/action_foodAlarmFragment_to_alarmRepeatFragment"
            app:destination="@id/foodAlarmRepeatFragment" />
    </dialog>

    <dialog
        android:id="@+id/foodAlarmRepeatFragment"
        android:name="com.example.mypet.ui.food.alarm.repeat.FoodAlarmRepeatFragment"
        tools:layout="@layout/fragment_food_alarm_repeat" />
</navigation>